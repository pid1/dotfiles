# only start a new session if we don't already have one
alias mux='tmux new-session -AD -s main'

# wrist savers
alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -i'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias ls="ls --color"
alias untar='tar xzvf'
alias tarup='tar czvf'
alias vi="nvim"
alias vim="nvim"